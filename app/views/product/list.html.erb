<h1>List Products</h1>
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>SKU</th>
      <th>Price</th>
      <th>Stock Qty</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.id %></td>
        <td><%= product.name %></td>
        <td><%= product.sku %></td>
        <td><%= product.price %></td>
        <td><%= product.stock_qty %></td>
        <td><%= link_to 'Edit', admin_product_edit_path(product) %></td>
      </tr>
    <% end %>
  </tbody>
</table>